{
  "name": "@flitz/cookies",
  "version": "1.0.2",
  "description": "flitz middleware for parsing cookies.",
  "private": false,
  "main": "lib/index.js",
  "files": [
    "index.d.ts",
    "lib/**/*",
    "!lib/sandbox/**/*",
    "!**/*.map",
    "!**/__tests__/**/*.+(ts|tsx|js)",
    "!lib/setupTests.*"
  ],
  "scripts": {
    "build": "del ./lib && tsc -p ./tsconfig.json",
    "lint": "eslint -c .eslintrc.js --ext .ts src",
    "lint:fix": "eslint --fix -c .eslintrc.js --ext .ts src",
    "sandbox": "ts-node-dev --respawn ./src/sandbox/index.ts",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flitz-js/cookies.git"
  },
  "keywords": [
    "flitz",
    "middleware",
    "cookie",
    "parser",
    "key values",
    "map",
    "dictionary",
    "typescript"
  ],
  "author": "Marcel Joachim Kloubert <marcel.kloubert@gmx.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/flitz-js/cookies/issues"
  },
  "homepage": "https://github.com/flitz-js/cookies#readme",
  "devDependencies": {
    "@flitz/tsconfig": "0.1.1",
    "@types/jest": "^26.0.5",
    "@types/node": "^10.17.27",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/eslint-plugin-tslint": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "del-cli": "^3.0.1",
    "eslint": "^7.5.0",
    "eslint-config-flitz": "0.1.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsdoc": "^30.0.3",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-unicorn": "^21.0.0",
    "flitz": "0.8.3",
    "jest": "^26.1.0",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.3",
    "ts-node-dev": "^1.0.0-pre.52",
    "tslint": "^6.1.2",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  }
}
